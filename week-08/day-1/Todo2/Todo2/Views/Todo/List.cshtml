@model System.Collections.Generic.List<Todo2.Models.Todo>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Todoos</title>
</head>
<body>
    <h1>Todos for today</h1>
    <br />
    <table>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.IsUrgent</td>
                <td>@item.IsDone</td>
            </tr>
        }
    </table>
</body>
</html>