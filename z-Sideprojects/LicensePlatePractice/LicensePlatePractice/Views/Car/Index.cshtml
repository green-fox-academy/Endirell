@model System.Collections.Generic.List<LicensePlatePractice.Models.Car>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
<head>
    <title>Cars</title>
    <meta charset="utf-8" />
</head>
<body>
    Body, body fire!
    <form action="/search" method="get">
        <input type="text" name="q" value="" />
        <button type="submit">Search</button>
        <a href="search?police=1">Police cars only</a>
        <a href="search?diplomat=1">Diplomats only</a>
    </form>
    @*<table border="1">*@
    <table border="1">
        <tr>
            <td>Licence plate</td>
            <td>Brand</td>
            <td>Model</td>
            <td>Color</td>
            <td>Year</td>
        </tr>
        @foreach (var car in @Model)
        {
            <tr>
                <td>@car.LicensePlate</td>
                <td>@car.CarBrand</td>
                <td>@car.CarModel</td>
                <td>@car.Color</td>
                <td>@car.Year</td>
            </tr>
        }
    </table>
</body>
</html>